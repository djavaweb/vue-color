    "vue": "^1.0.12"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "inline-loader": "^0.1.1",
    "node-sass": "^3.10.0",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "sass-loader": "^4.0.2",
