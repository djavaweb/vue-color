{
  "name": "vue-sketch-color-picker",
  "version": "0.0.1",
  "description": "Vue Sketch Color Picker Component",
  "keywords": [
    "color",
    "vuejs"
  ],
  "main": "dist/vue-sketch-color-picker.min.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config build/webpack.prod.config.js",
